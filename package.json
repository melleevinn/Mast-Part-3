{
  "name": "ChristoffelApp",
  "version": "1.0.0",
  "description": "A React Native app for managing menu items and checkout.",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ChristoffelApp.git"
